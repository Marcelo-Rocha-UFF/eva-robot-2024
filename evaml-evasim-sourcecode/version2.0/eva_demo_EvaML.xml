<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="eva_demo" xsi:noNamespaceSchemaLocation="evaml-schema/evaml_schema.xsd" id="fb74fb37ee6f50390462a0fe1e980687">
  <settings>
    <voice tone="en-US_EmmaExpressive" key="1000" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    
    <light state="OFF" key="1001" />
    <evaEmotion emotion="NEUTRAL" key="1002" />
    <led animation="STOP" key="1003" />
    <motion head="CENTER" right-arm="POSITION 0" left-arm="POSITION 0" key="1004" />
    <talk key="1005">Now I will initiate a test of all my capabilities</talk>


    
    <talk key="1006">I will start testing the smart bulb</talk>
    <audio source="efx-display" block="FALSE" key="1007" />
    <light state="ON" color="WHITE" key="1008" />
    <wait duration="1000" key="1009" />
    <audio source="efx-display" block="FALSE" key="1010" />
    <light state="ON" color="BLUE" key="1011" />
    <wait duration="1000" key="1012" />
    <audio source="efx-display" block="FALSE" key="1013" />
    <light state="ON" color="GREEN" key="1014" />
    <wait duration="1000" key="1015" />
    <audio source="efx-display" block="FALSE" key="1016" />
    <light state="ON" color="RED" key="1017" />
    <wait duration="1000" key="1018" />
    <audio source="efx-display" block="FALSE" key="1019" />
    <light state="ON" color="YELLOW" key="1020" />
    <wait duration="1000" key="1021" />


    
    <talk key="1022">I can express emotions using my gaze and I will test it now</talk>
    <audio source="efx-blin2" block="FALSE" key="1023" />
    <evaEmotion emotion="HAPPY" key="1024" />
    <wait duration="2000" key="1025" />
    <audio source="efx-blin2" block="FALSE" key="1026" />
    <evaEmotion emotion="SAD" key="1027" />
    <wait duration="2000" key="1028" />
    <audio source="efx-blin2" block="FALSE" key="1029" />
    <evaEmotion emotion="ANGRY" key="1030" />
    <wait duration="2000" key="1031" />
    <audio source="efx-blin2" block="FALSE" key="1032" />
    <evaEmotion emotion="FEAR" key="1033" />
    <wait duration="2000" key="1034" />
    <audio source="efx-blin2" block="FALSE" key="1035" />
    <evaEmotion emotion="SURPRISE" key="1036" />
    <wait duration="2000" key="1037" />
    <audio source="efx-blin2" block="FALSE" key="1038" />
    <evaEmotion emotion="DISGUST" key="1039" />
    <wait duration="2000" key="1040" />
    <audio source="efx-blin2" block="FALSE" key="1041" />
    <evaEmotion emotion="INLOVE" key="1042" />
    <wait duration="2000" key="1043" />


    
    <evaEmotion emotion="NEUTRAL" key="1044" />
    <talk key="1045">Now, I will run some animations using the leds on my chest</talk>
    <audio source="efx-robot" block="FALSE" key="1046" />
    <light state="ON" color="GREEN" key="1047" />
    <led animation="HAPPY" key="1048" />
    <wait duration="5000" key="1049" />
    <audio source="efx-robot" block="FALSE" key="1050" />
    <light state="ON" color="RED" key="1051" />
    <led animation="ANGRY" key="1052" />
    <wait duration="5000" key="1053" />
    <audio source="efx-robot" block="FALSE" key="1054" />
    <light state="ON" color="YELLOW" key="1055" />
    <led animation="SURPRISE" key="1056" />
    <wait duration="5000" key="1057" />
    <audio source="efx-robot" block="FALSE" key="1058" />
    <light state="ON" color="WHITE" key="1059" />
    <led animation="RAINBOW" key="1060" />
    <wait duration="5000" key="1061" />

    
    <led animation="STOP" key="1062" />
    <talk key="1063">I have non verbal capabilities so I can move my head</talk>
    <wait duration="1000" key="1064" />
    <talk key="1065">I can move up</talk>
    <audio source="efx-robot-head" block="FALSE" key="1066" />
    <motion type="2UP" key="1067" />
    <wait duration="2000" key="1068" />
    <motion type="CENTER" key="1069" />
    <wait duration="2000" key="1070" />
    <talk key="1071">I can move down</talk>
    <audio source="efx-robot-head" block="FALSE" key="1072" />
    <motion type="2DOWN" key="1073" />
    <wait duration="2000" key="1074" />
    <motion type="CENTER" key="1075" />
    <wait duration="2000" key="1076" />
    <talk key="1077">I can say no</talk>
    <light state="ON" color="RED" key="1078" />
    <led animation="ANGRY" key="1079" />
    <evaEmotion emotion="ANGRY" key="1080" />
    <motion head="DOWN" key="1081" />
    <wait duration="1000" key="1082" />
    <motion type="NO" key="1083" />
    <wait duration="4000" key="1084" />
    <talk key="1085">and I can say yes</talk>
    <led animation="HAPPY" key="1086" />
    <light state="ON" color="GREEN" key="1087" />
    <evaEmotion emotion="HAPPY" key="1088" />
    <motion type="2YES" key="1089" />
    <wait duration="4000" key="1090" />
    <evaEmotion emotion="NEUTRAL" key="1091" />
    <motion head="CENTER" key="1092" />
    <light state="ON" color="WHITE" key="1093" />
    
    
    <talk key="1094">I can move my right arm</talk>
    <audio source="efx-robot-arm" block="FALSE" key="1095" />
    <motion right-arm="POSITION 3" key="1096" />
    <wait duration="2000" key="1097" />
    <talk key="1098">I can move my left arm</talk>
    <audio source="efx-robot-arm" block="FALSE" key="1099" />
    <motion left-arm="POSITION 2" key="1100" />
    <wait duration="2000" key="1101" />
    <talk key="1102">and I can shake my arms</talk>
    <audio source="song-samba-loop" block="FALSE" key="1103" />
    <motion left-arm="SHAKE2" right-arm="SHAKE2" key="1104" />
    <wait duration="6000" key="1105" />
    <motion right-arm="POSITION 0" left-arm="POSITION 0" key="1106" />

    
    <talk key="1107">I can also play sounds</talk>
    <audio source="efx-blin" block="TRUE" key="1108" />
    <talk key="1109">I can play some good music too</talk>
    <audio source="song-rude-magic" block="FALSE" key="1110" />
    <led animation="SURPRISE" key="1111" />
    <motion head="RIGHT_UP2" right-arm="POSITION 3" left-arm="POSITION 0" key="1112" />
    <evaEmotion emotion="HAPPY" key="1113" />
    <wait duration="7000" key="1114" />
    <motion head="LEFT_UP2" right-arm="POSITION 0" left-arm="POSITION 3" key="1115" />
    <wait duration="7000" key="1116" />
    <motion head="CENTER" right-arm="POSITION 0" left-arm="POSITION 0" key="1117" />

    
    <led animation="STOP" key="1118" />
    <motion head="CENTER" key="1119" />
    <evaEmotion emotion="NEUTRAL" key="1120" />
    <talk key="1121">Please, could you say your name?</talk>
    <listen key="1122" />
    <talk key="1123">Hello $</talk>
    <talk key="1124">How are you?</talk>
    <wait duration="2000" key="1125" />
    <talk key="1126">Good bye!</talk>
    <light state="OFF" key="1127" />
  </script>
