      <motion head="CENTER" key="1048" />
      <evaEmotion emotion="NEUTRAL" key="1049" />
      <light state="OFF" key="1050" />
    </case>
      <default key="1051" child_proc="true" value="" op="exact">
        <random min="1" max="2" var="TIPO_FIM" key="1052" />
        <switch var="TIPO_FIM">
          <case op="eq" value="1" key="1053" child_proc="true" var="TIPO_FIM">
            <light state="ON" color="PINK" key="1054" />
      <evaEmotion emotion="INLOVE" key="1055" />
      <led animation="ANGRY" key="1056" />
      <talk key="1057">Sim, eu lembro de você!</talk>
      <audio source="efx-blin2" block="TRUE" key="1058" />
      <motion head="DOWN1" key="1059" />
      <talk key="1060">Você se chãma $</talk>
      <led animation="RAINBOW" key="1061" />
      <motion head="2NO" left-arm="SHAKE2" right-arm="SHAKE2" key="1062" />
      <audio source="efx-aplausos-bom" block="TRUE" key="1063" />
      <evaEmotion emotion="NEUTRAL" key="1064" />
      <motion head="CENTER" key="1065" />
      <talk key="1066">$, muito obrigado por ter participado do teste!</talk>
      <light state="OFF" key="1067" />
    </case>
          <case op="eq" value="2" key="1068" child_proc="true" var="TIPO_FIM">
