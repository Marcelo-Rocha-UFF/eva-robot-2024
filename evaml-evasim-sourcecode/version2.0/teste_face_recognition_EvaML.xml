      <evaEmotion emotion="INLOVE" key="1070" />
      <led animation="ANGRY" key="1071" />
      <talk key="1072">Lembro muito bem de você!</talk>
      <audio source="efx-blin2" block="TRUE" key="1073" />
      <motion head="DOWN1" key="1074" />
      <talk key="1075">Seu nome é $</talk>
      <led animation="RAINBOW" key="1076" />
      <motion head="2NO" left-arm="SHAKE2" right-arm="SHAKE2" key="1077" />
      <audio source="efx-aplausos-bom" block="TRUE" key="1078" />
      <evaEmotion emotion="NEUTRAL" key="1079" />
      <motion head="CENTER" key="1080" />
      <talk key="1081">$, muito obrigado por ter participado do teste!</talk>
      <light state="OFF" key="1082" />
    </case>
